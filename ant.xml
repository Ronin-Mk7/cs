<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<project default="build" name="cs01" basedir=".">
    <property name="dir" location="CS Labs/build/cs01/1PachachuraA"/>
    <property name="cleandir" location="CS Labs/build/cs01"/>

    <!--Build section                                        -->
    <target name="build"  description="build files">
        <jar destfile="${dir}PrintNameAndTown.jar" filesetmanifest="mergewithoutmain">
            <manifest>
                <attribute name="Main-Class" value="cs01printinganddebugging.PrintNameAndTown"/>
                <attribute name="Class-Path" value="."/>
            </manifest>
            <fileset dir="CS Labs/bin"/>
            <fileset dir="CS Libs/bin"/>
        </jar>
	<jar destfile="${dir}PrintMonth.jar" filesetmanifest="mergewithoutmain">
            <manifest>
                <attribute name="Main-Class" value="cs01printinganddebugging.PrintMonth"/>
                <attribute name="Class-Path" value="."/>
            </manifest>
            <fileset dir="CS Labs/bin"/>
            <fileset dir="CS Libs/bin"/>
        </jar>
	<jar destfile="${dir}PrintBio.jar" filesetmanifest="mergewithoutmain">
            <manifest>
                <attribute name="Main-Class" value="cs01printinganddebugging.PrintBio"/>
                <attribute name="Class-Path" value="."/>
            </manifest>
            <fileset dir="CS Labs/bin"/>
            <fileset dir="CS Libs/bin"/>
        </jar>
    </target>

    <!--Clean section                                        -->
    <target name="clean" description="remove unnecessary files">
    <delete dir="${cleandir}"/>
    </target>

</project>
